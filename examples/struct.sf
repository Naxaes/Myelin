import * from macos
import * from core

foo := Foo { bar=11, baz="Hello ted!\n" }
x, y := display_foo(foo)

print_int(x)
print_int(y)

display_foo: (obj: Foo) -> int, int {
	return obj.bar, temp(obj)

	temp: (thing: Foo) -> int {
		print(thing.baz, thing.bar)
		return thing.bar + 10
	}
}

Foo: struct {
	bar: int
	baz: str
}

exit(x + y)