import * from macos
import * from core

a := "Hello world!\n"
print(a, a.len)

message := alloc(32) as str
copy(message,   "Hello ", 6)
copy(message+6, "World ", 6)

i := 0
while i < 10 {
	message[i + 12] = 48 + i
	i = i + 1
}
message[12 + i]     = 10
message[12 + i + 1] = 0

print(message, 23)


